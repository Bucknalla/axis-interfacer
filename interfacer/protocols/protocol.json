{
    "VERSION":"0.0.1",
    "PROTOCOLS":{
        "SIGNAL":{
            "CLOCK":{
                "PARAMETERS":{
                    "PRAGMA": "(* X_INTERFACE_PARAMETER = \"ASSOCIATED_BUSIF {0}, ASSOCIATED_RESET {1}, FREQ_HZ {2}\" *)"
                },
                "INFO":{
                    "PRAGMA": "(* X_INTERFACE_INFO = \"xilinx.com:signal:clock:1.0 {0} {1}\" *)",
                    "INTERFACE":{
                        "CLK":{
                            "REQUIRED": true,
                            "ALTERNATIVES":["clk","clock"]
                        }                    
                    }
                }
            },
            "CLOCK_ENABLE":{
                "PARAMETERS":{
                    "PRAGMA": "(* X_INTERFACE_PARAMETER = \"POLARITY {0}\" *)"
                    },
                "INFO":{
                    "PRAGMA": "(* X_INTERFACE_INFO = \"xilinx.com:signal:clockenable:1.0 {0} {1}\" *)",
                    "INTERFACE":{
                        "CE":{
                            "REQUIRED": true,
                            "ALTERNATIVES":["ce","clock_enable"]
                        }                    
                    }
                }
            },
            "RESET":{
                "PARAMETERS":{
                    "PRAGMA": "(* X_INTERFACE_PARAMETER = \"POLARITY {0}\" *)"
                },
                "INFO":{
                    "PRAGMA": "(* X_INTERFACE_INFO = \"xilinx.com:signal:reset:1.0 {0} {1}\" *)",
                    "INTERFACE":{
                        "RST":{
                            "REQUIRED": true,
                            "ALTERNATIVES":["rst","reset"]
                        }                    
                    }
                }
            },
            "INTERRUPT":{
                "PARAMETERS":{
                    "PRAGMA": "(* X_INTERFACE_PARAMETER = \"SENSITIVITY {0}\" *)"
                },
                "INFO":{
                    "PRAGMA": "(* X_INTERFACE_INFO = \"xilinx.com:signal:interrupt:1.0 {0} {1}\" *)",
                    "INTERFACE":{
                        "INTERRUPT":{
                            "REQUIRED": false,
                            "ALTERNATIVES":["irq","interrupt"]
                        }                    
                    }
                }
            }
        },
        "AXI":{
            "STREAM":{
                "PARAMETERS":{
                    "PRAGMA": "(* X_INTERFACE_PARAMETER = \"{0}\" *)",
                    "PARAM":{
                        "CLK_DOMAIN": "string",
                        "PHASE": "float",
                        "FREQ_HZ": "float",
                        "LAYERED_METADATA": "string",
                        "HAS_TLAST": "bool", 
                        "HAS_TKEEP": "bool",
                        "HAS_TSTRB": "bool",
                        "HAS_TREADY": "bool",
                        "TUSER_WIDTH": "int",
                        "TID_WIDTH": "int",
                        "TDEST_WIDTH": "int",
                        "TDATA_NUM_BYTES": "int"
                    }
                },
                "INFO":{
                    "PRAGMA": "(* X_INTERFACE_INFO = \"xilinx.com:interface:axis:1.0 {0} {1}\" *)",
                    "INTERFACE":{
                        "TID":{
                            "REQUIRED": false
                        },
                        "TDEST":{
                            "REQUIRED": false
                        },
                        "TDATA":{
                            "REQUIRED": true
                        },
                        "TSTRB":{
                            "REQUIRED": false
                        },
                        "TKEEP":{
                            "REQUIRED": false
                        },
                        "TLAST":{
                            "REQUIRED": false
                        },
                        "TUSER":{
                            "REQUIRED": false
                        },
                        "TVALID":{
                            "REQUIRED": true
                        },
                        "TREADY":{
                            "REQUIRED": true
                        }
                    }
                }
            }
        }
    }
}